<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Equipos" table="equipos">
    <unique-constraints>
      <unique-constraint name="nombre" columns="nombre"/>
    </unique-constraints>
    <id name="id" type="bigint" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-many field="jugadores" target-entity="Jugadores" mapped-by="equipos" />
  </entity>
</doctrine-mapping>
